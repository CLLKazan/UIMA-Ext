					<description>Axxx</description>
				</value>
				<value>
					<string>SW</string>
					<description>zzzzzz</description>
				</value>
				<value>
					<string>CAP</string>
					<description>AAAAA</description>
				</value>

				<value>
					<string>MISC</string>
					<description>asdWcsA</description>
				</value>

			</allowedValues>
