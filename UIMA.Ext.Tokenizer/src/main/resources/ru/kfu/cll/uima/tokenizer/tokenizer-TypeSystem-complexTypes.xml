			<supertypeName>uima.cas.String</supertypeName>
			<allowedValues>
				<value>
					<string>Unsigned</string>
					<description>123</description>
				</value>
				<value>
					<string>Negative</string>
					<description>-123</description>
				</value>
			</allowedValues>
