<?xml version="1.0" encoding="UTF-8" ?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<frameworkImplementation>org.apache.uima.java</frameworkImplementation>
	<primitive>false</primitive>

	<delegateAnalysisEngineSpecifiers>
		<delegateAnalysisEngine key="tokenizer">
			<import name="WhitespaceTokenizer" />
		</delegateAnalysisEngine>
		<delegateAnalysisEngine key="morphAnnotator">
			<import
				name="ru.ksu.niimm.cll.uima.morph.opencorpora.an-ru-MorphAnnotator" />
		</delegateAnalysisEngine>
		<delegateAnalysisEngine key="xmiWriter">
			<import name="ru.kfu.itis.cll.uima.commons.an-XMIWriter"/>
		</delegateAnalysisEngine>
	</delegateAnalysisEngineSpecifiers>

	<analysisEngineMetaData>
		<name>Aggregate AE for testing</name>
		<configurationParameters>
			<configurationParameter>
				<name>TokenType</name>
				<description />
				<type>String</type>
				<mandatory>true</mandatory>
				<overrides>
					<parameter>morphAnnotator/TokenType</parameter>
				</overrides>
			</configurationParameter>
			<configurationParameter>
				<name>XmiOutputDir</name>
				<type>String</type>
				<overrides>
					<parameter>xmiWriter/OutputDirectory</parameter>
				</overrides>
			</configurationParameter>
		</configurationParameters>
		<configurationParameterSettings>
			<nameValuePair>
				<name>TokenType</name>
				<value>
					<string>org.apache.uima.TokenAnnotation</string>
				</value>
			</nameValuePair>
			<nameValuePair>
				<name>XmiOutputDir</name>
				<value>
					<string>:OVERRIDE-PROGRAMMATICALLY:</string>
				</value>
			</nameValuePair>
		</configurationParameterSettings>
		<flowConstraints>
			<fixedFlow>
				<node>tokenizer</node>
				<node>morphAnnotator</node>
				<node>xmiWriter</node>
			</fixedFlow>
		</flowConstraints>
	</analysisEngineMetaData>

	<resourceManagerConfiguration>
		<externalResources>
			<externalResource>
				<name>SerializedOpenCorporaMorphDictionary</name>
				<description />
				<fileResourceSpecifier>
					<fileUrl>file:${opencorpora.morph.dictionary.serialized}</fileUrl>
				</fileResourceSpecifier>
				<implementationName>ru.ksu.niimm.cll.uima.morph.opencorpora.resource.SerializedDictionaryResource</implementationName>
			</externalResource>
		</externalResources>
		<externalResourceBindings>
			<externalResourceBinding>
				<key>morphAnnotator/MorphDictionary</key>
				<resourceName>SerializedOpenCorporaMorphDictionary</resourceName>
			</externalResourceBinding>
		</externalResourceBindings>
	</resourceManagerConfiguration>
</analysisEngineDescription>