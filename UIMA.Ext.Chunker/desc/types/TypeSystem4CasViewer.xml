<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <types>
        <typeDescription>
            <name>org.opencorpora.cas.Word</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>wordforms</name>
                    <description/>
                    <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                    <elementType>org.opencorpora.cas.Wordform</elementType>
                </featureDescription>
            </features>
        </typeDescription>
        <typeDescription>
            <name>org.opencorpora.cas.Wordform</name>
            <description/>
            <supertypeName>uima.cas.TOP</supertypeName>
            <features>
                <featureDescription>
                    <name>pos</name>
                    <description/>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>lemma</name>
                    <description/>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>lemmaId</name>
                    <description/>
                    <rangeTypeName>uima.cas.Integer</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>grammems</name>
                    <description/>
                    <rangeTypeName>uima.cas.StringArray</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>
        <typeDescription>
            <name>ru.kfu.itis.cll.uima.commons.DocumentMetadata</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>sourceUri</name>
                    <description/>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>offsetInSource</name>
                    <description/>
                    <rangeTypeName>uima.cas.Long</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>documentSize</name>
                    <description/>
                    <rangeTypeName>uima.cas.Long</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>startProcessingTime</name>
                    <description/>
                    <rangeTypeName>uima.cas.Long</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>
        <typeDescription>
            <name>ru.kfu.itis.issst.uima.chunker.cas.Chunk</name>
            <description>Represents typed 'chunk', i.e. a head word with its dependents.
				Chunk annotation should have the same borders with its head word.</description>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>chunkType</name>
                    <description/>
                    <rangeTypeName>ru.kfu.itis.issst.uima.chunker.cas.ChunkType</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>head</name>
                    <description/>
                    <rangeTypeName>org.opencorpora.cas.Word</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>dependents</name>
                    <description/>
                    <rangeTypeName>uima.cas.FSArray</rangeTypeName>
                    <elementType>org.opencorpora.cas.Word</elementType>
                </featureDescription>
            </features>
        </typeDescription>
        <typeDescription>
            <name>ru.kfu.itis.issst.uima.chunker.cas.ChunkType</name>
            <description/>
            <supertypeName>uima.cas.String</supertypeName>
            <allowedValues>
                <value>
                    <string>VP</string>
                    <description/>
                </value>
                <value>
                    <string>NP</string>
                    <description/>
                </value>
            </allowedValues>
        </typeDescription>
    </types>
</typeSystemDescription>
