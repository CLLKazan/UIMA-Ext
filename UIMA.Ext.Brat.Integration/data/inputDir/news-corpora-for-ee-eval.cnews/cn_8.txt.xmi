<?xml version="1.0" encoding="UTF-8"?><xmi:XMI xmlns:typesystem="http:///com/hp/hplabs/lim2/ie/text/typesystem.ecore" xmlns:cas="http:///uima/cas.ecore" xmlns:temporal="http:///com/hp/hplabs/lim2/ie/text/typesystem/temporal.ecore" xmlns:type2="http:///de/uniwue/tm/type.ecore" xmlns:xmi="http://www.omg.org/XMI" xmlns:typesystem2="http:///com/hp/hpl/typesystem.ecore" xmlns:tcas="http:///uima/tcas.ecore" xmlns:type="http:///de/uniwue/tm/textmarker/kernel/type.ecore" xmi:version="2.0">
