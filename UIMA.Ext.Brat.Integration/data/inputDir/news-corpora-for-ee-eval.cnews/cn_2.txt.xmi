<?xml version="1.0" encoding="UTF-8"?><xmi:XMI xmlns:typesystem="http:///ru/kfu/itis/issst/ner/typesystem.ecore" xmlns:cas="http:///uima/cas.ecore" xmlns:temporal="http:///ru/kfu/itis/issst/ner/typesystem/temporal.ecore" xmlns:type2="http:///de/uniwue/tm/type.ecore" xmlns:xmi="http://www.omg.org/XMI" xmlns:typesystem2="http:///ru/kfu/itis/issst/ner/typesystem.ecore" xmlns:typesystem3="http:///ru/kfu/itis/issst/ner/typesystem.ecore" xmlns:tcas="http:///uima/tcas.ecore" xmlns:type="http:///de/uniwue/tm/textmarker/kernel/type.ecore" xmi:version="2.0">
