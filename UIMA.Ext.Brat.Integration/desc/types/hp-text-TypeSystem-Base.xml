<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>hp-text-TypeSystem-Base</name>
  <description/>
  <version>1.0</version>
  <vendor>HP Labs</vendor>
  <types>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</name>
      <description>.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>annotatorID</name>
          <description>The identifier of the annotator made this annotation. Can be null.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence of the annotation. Zero (0) or negative value means that this value is not available for the annotation.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.Sentence</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.Token</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>posTag</name>
          <description>Contains part-of-speech of a corresponding token</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>SemClass</name>
          <description>semantic class of token</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>POS</name>
          <description>Part of SPeech of term to which this
								token is a part</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>frost_TokenType</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>stemForm</name>
          <description>Stemmed form of a token.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.Chunk</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>chunkType</name>
          <description>The type of chunk (NP, VP, PP...)</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.NamedEntity</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>namedEntityType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
        	<name>canonical</name>
        	<description/>
        	<rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.ParseNode</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>parentNode</name>
          <description/>
          <rangeTypeName>com.hp.hplabs.lim2.ie.text.typesystem.ParseNode</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>parseType</name>
          <description>Constituent label for this node of the parse.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>childrenNodes</name>
          <description>The children nodes of this parse node</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.hp.hplabs.lim2.ie.text.typesystem.ParseNode</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hplabs.lim2.ie.text.typesystem.TopParseNode</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.ParseNode</supertypeName>
      <features>
        <featureDescription>
          <name>parseView</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.hp.hpl.typesystem.DocumentMetadata</name>
      <description/>
      <supertypeName>com.hp.hplabs.lim2.ie.text.typesystem.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>uri</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>offset</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>documentSize</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lastSegment</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>title</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>encoding</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>startProcessingTicks</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>stopProcessingTicks</name>
          <description/>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tags</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>classes</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
