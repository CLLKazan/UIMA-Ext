<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<frameworkImplementation>org.apache.uima.java</frameworkImplementation>
	<primitive>false</primitive>
	<delegateAnalysisEngineSpecifiers>
		<delegateAnalysisEngine key="aggregate-AllEvents">
			<import location="ae-ru-AllEvents.xml" />
		</delegateAnalysisEngine>
		<delegateAnalysisEngine key="JdbcWriter">
			<import location="../consumers/hplabs/con-ru-JdbcWriter.xml" />
		</delegateAnalysisEngine>
	</delegateAnalysisEngineSpecifiers>
	<analysisEngineMetaData>
		<name>ae-ru-AllEvents+out</name>
		<description />
		<version>1.0</version>
		<vendor />
		<configurationParameters searchStrategy="language_fallback" />
		<configurationParameterSettings />
		<flowConstraints>
			<fixedFlow>
				<node>aggregate-AllEvents</node>
				<node>JdbcWriter</node>
			</fixedFlow>
		</flowConstraints>
		<fsIndexCollection />
		<capabilities>
			<capability>
				<inputs />
				<outputs />
				<languagesSupported />
			</capability>
		</capabilities>
		<operationalProperties>
			<modifiesCas>true</modifiesCas>
			<multipleDeploymentAllowed>true</multipleDeploymentAllowed>
			<outputsNewCASes>false</outputsNewCASes>
		</operationalProperties>
	</analysisEngineMetaData>
	<resourceManagerConfiguration>
		<externalResources>
	  		<!-- 
	  		<externalResource>
	  			<name>CasFilePrinterOut</name>
	  			<description/>
	  			<fileResourceSpecifier>
	  				<fileUrl>file:data/experiments/cas-file-printer.properties</fileUrl>
	  			</fileResourceSpecifier>			
	  			<implementationName>com.hp.hpl.events.uima.consumers.cao.FilePrinterCasAccessObject</implementationName>
	  		</externalResource>
	  		 -->
			<externalResource>
				<name>CasMysqlDataSource</name>
				<description />
				<fileResourceSpecifier>
					<fileUrl>file:data/experiments/cas-mysql-ds.properties</fileUrl>
				</fileResourceSpecifier>
				<implementationName>com.hp.hpl.events.uima.consumers.cao.MysqlJdbcCasAccessObject</implementationName>
			</externalResource>
		</externalResources>
		<externalResourceBindings>
			<externalResourceBinding>
				<key>JdbcWriter/CasAccessObject</key>
				<resourceName>CasMysqlDataSource</resourceName>
			</externalResourceBinding>
		</externalResourceBindings>
	</resourceManagerConfiguration>
</analysisEngineDescription>
